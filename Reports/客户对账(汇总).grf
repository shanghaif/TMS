						Styles=[DrawLeft|DrawRight|DrawBottom]
					End
					DataField='应收金额'
				End
				Item
					Type=SummaryBox
					Name='SummaryBox4'
					AlignColumn='票数'
					Left=15.1342
					Width=3.83646
					Height=0.687917
					Object Border
						Styles=[DrawLeft|DrawRight|DrawBottom]
					End
					DataField='单数'
				End
				Item
					Type=SummaryBox
					Name='SummaryBox5'
					AlignColumn='代收货款'
					Left=11.14
					Width=4.02167
					Height=0.687917
					Object Border
						Styles=[DrawLeft|DrawRight|DrawBottom]
					End
					DataField='代收货款'
				End
			End
