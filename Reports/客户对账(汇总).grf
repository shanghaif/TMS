Object Report
	Version='6.0.0.8'
	Title='送货费核销报表'
	PrintAsDesignPaper=F
	Object Font
		Name='宋体'
		Size=105000,0
		Weight=400
		Charset=134
	End
	Object Printer
		Height=29.7
		LeftMargin=1
		TopMargin=2
		RightMargin=1
		BottomMargin=2
	End
	Object DetailGrid
		Object Font
			Name='宋体'
			Weight=400
			Charset=134
		End
		CenterView=T
		Object Recordset
			Items Field
				Item
					Name='开始时间'
					Format='yyyy/MM/dd'
					DBFieldName='bdate'
				End
				Item
					Name='结束时间'
					Format='yyyy/MM/dd'
					DBFieldName='edate'
				End
				Item
					Name='客户名称'
					DBFieldName='shipper'
				End
				Item
					Name='应收金额'
					Type=Float
					Format='0;;#'
					DBFieldName='account'
				End
				Item
					Name='代收货款'
					Type=Float
					Format='0;;#'
					DBFieldName='accdaishou'
				End
				Item
					Name='单数'
					Type=Integer
					DBFieldName='numbers'
				End
			End
		End
		Items Column
			Item
				Name='序号'
				Width=2.96333
			End
			Item
				Name='客户名称'
				Width=4.20688
			End
			Item
				Name='应收金额'
				Width=4
			End
			Item
				Name='代收货款'
				Width=4
			End
			Item
				Name='票数'
				Width=3.81
			End
		End
		Object ColumnContent
			Height=0.608542
			Items ColumnContentCell
				Item
					Column='序号'
					FreeCell=T
					Items Control
						Item
							Type=SystemVarBox
							Name='SystemVarBox1'
							Dock=Fill
							SystemVar=RowNo
						End
					End
				End
				Item
					Column='客户名称'
					WordWrap=T
					DataField='客户名称'
				End
				Item
					Column='应收金额'
					WordWrap=T
					TextAlign=MiddleRight
					DataField='应收金额'
				End
				Item
					Column='代收货款'
					WordWrap=T
					TextAlign=MiddleRight
					DataField='代收货款'
				End
				Item
					Column='票数'
					WordWrap=T
					TextAlign=MiddleRight
					DataField='单数'
				End
			End
		End
		Object ColumnTitle
			Height=0.79375
			RepeatStyle=OnPage
			Items ColumnTitleCell
				Item
					GroupTitle=F
					Column='序号'
					TextAlign=MiddleCenter
					Text='序号'
				End
				Item
					GroupTitle=F
					Column='客户名称'
					WordWrap=T
					TextAlign=MiddleCenter
					Text='客户名称'
				End
				Item
					GroupTitle=F
					Column='应收金额'
					WordWrap=T
					TextAlign=MiddleCenter
					Text='应收金额'
				End
				Item
					GroupTitle=F
					Column='代收货款'
					WordWrap=T
					TextAlign=MiddleCenter
					Text='代收货款'
				End
				Item
					GroupTitle=F
					Column='票数'
					WordWrap=T
					TextAlign=MiddleCenter
					Text='票数'
				End
			End
		End
	End
